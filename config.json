{
	"mode": "development",
	"webServer": {
		"host": "0.0.0.0",
		"port": 8888,
		"docsRootUrl": "/",
		"apiUrl": "/api/v1/docs/"
	},

	"docs": {
		"path": "./doc",
		"encoding": "utf-8",
		"newDirMask": "0755",
		"extension": ".md",
		"indexName": "index"
	},

	"log": {
		"fileName":"./webdoc.log",
		"fileMaxSize": 10485760,
		"filesMaxCount": 10
	},

	"title": "WebDoc Portal for iFoxy",
	"logo": "/img/logo.png",
	"hideNavigation": false,
	"readOnly": false,
	"moderators": [
		"localhost",
		"ion",
		"127.0.0.1"
	]
}
